<?php
// [
//     [boxId: 5, amont 10],
//     [boxId: 45, amont 10],
//     [boxId: 45, amont 150],
// ]

$boxes = [];

for ($i = 0; $i < 17; $i++) {
    $boxes[] = [
        'boxId' => rand(10000000, 99999999),
        'amount' => rand(1, 1000),
    ];
}


file_put_contents(__DIR__ . '/../data/boxes.json', json_encode($boxes, JSON_PRETTY_PRINT));